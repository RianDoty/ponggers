{
  "name": "ponggers",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@vitejs/plugin-react": "2.1.0",
    "concurrently": "^6.3.0",
    "express": "^4.17.1",
    "prop-types": "15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.3.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "scripts": {
    "start": "npm run developmentVSC",
    "development": "NODE_ENV=development concurrently --kill-others \"npm run client\" \"npm run server\"",
